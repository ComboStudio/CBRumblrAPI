{
  "name": "rumblr-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./lib/server.js --exec babel-node --presets es2015",
    "watch": "webpack --config webpack.config.dev.js --watch --progress",
    "build": "babel lib -d build --presets es2015",
    "prod": "node ./build/server.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "q": "^1.5.0",
    "sonos": "^1.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-watch": "^2.0.2",
    "node-sass": "^3.9.3",
    "nodemon": "^1.10.2",
    "webpack": "^1.13.2"
  }
}
