{
  "name": "sams-awesome-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./server.js --exec babel-node --presets es2015",
    "build": "babel src -d build",
    "prod": "node ./build/server.js",
    "watch": "webpack --config webpack.config.dev.js --watch --progress"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "q": "^1.5.0",
    "sonos": "^0.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-watch": "^2.0.2",
    "node-sass": "^3.9.3",
    "nodemon": "^1.10.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "webpack": "^1.13.2",
    "css-loader": "^0.26.2",
    "style-loader": "^0.13.2",
    "file-loader": "^0.10.1"
